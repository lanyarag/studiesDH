@{
    ActorModel Actor = new ActorModel();
}
<div class="text-center">
    <h1 class="display-4">Movie Catalog</h1>
    <table class="table"> 
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Name</th>
                <th scope="col">Last Name</th>
                <th scope="col">Movies</th>
             </tr>
        </thead>
        <tbody>
             <tr>
              <th scope="row">@Actor.Id</th>
              <td>@Actor.Name</td>
              <td>@Actor.LastName</td>
              @*<td>@Actor.Movies</td>*@
            </tr>
    </table>
</div>
